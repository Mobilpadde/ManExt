{
	"manifest_version": 2,

	"name": "ManExt",
	"description": "Manage - and group - your installed extensions",
	"version": "1.2",

	"author": "Mads Cordes",
	"icons": {
		"128": "assets/img/icon_128.png"
	},

	"permissions": [
		"management",
		"storage",
		"tabs"
	],
	"browser_action": {
		"default_icon": "assets/img/icon.png",
		"default_popup": "popup.html"
	}
}